{
  "name": "assoconnect/rudderstack-bundle",
  "type": "symfony-bundle",
  "description": "Rudderstack php lib",
  "keywords": [
    "assoconnect",
    "rudderstack",
    "bundle",
    "symfony"
  ],
  "homepage": "http://github.com/assoconnect/RudderstackBundle",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^7.4|^8.0",
    "rudderstack/rudder-php-sdk": "^1.0",
    "psr/log": "^1.0",
    "symfony/config": "^2.7",
    "symfony/dependency-injection": "^2.7",
    "symfony/http-kernel": "^2.7",
    "symfony/security-core": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "squizlabs/php_codesniffer": "^3",
    "phpstan/phpstan": "^1.2",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.1",
    "roave/no-floaters": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "AssoConnect\\RudderstackBundle\\": "src/"
    }
  }
}
